<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CameraView"><attr format="enum" name="ckFacing">
            <enum name="back" value="0"/>
            <enum name="front" value="1"/>
        </attr><attr format="enum" name="ckFlash">
            <enum name="off" value="0"/>
            <enum name="on" value="1"/>
            <enum name="auto" value="2"/>
        </attr><attr format="enum" name="ckFocus">
            <enum name="off" value="0"/>
            <enum name="continuous" value="1"/>

            
            <enum name="tap" value="1"/>
        </attr><attr format="enum" name="ckZoom">
            <enum name="off" value="0"/>

            
            <enum name="pinch" value="0"/>
        </attr><attr format="enum" name="ckMethod">
            <enum name="standard" value="0"/>
            <enum name="still" value="1"/>

            
            <enum name="speed" value="0"/>
        </attr><attr format="integer" name="ckJpegQuality"/><attr format="boolean" name="ckCropOutput"/><attr name="android:adjustViewBounds"/></declare-styleable>
</resources>